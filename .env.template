COOKIE_SECRET=supersecret # TODO GENERATE SECURE SECRET
JWT_SECRET=supersecret  # TODO GENERATE SECURE SECRET

# CORS settings - Update for production domains
# For production, replace localhost with your actual domain
STORE_CORS=http://localhost:8000,https://docs.medusajs.com
ADMIN_CORS=http://localhost:5173,http://localhost:9000,https://docs.medusajs.com
AUTH_CORS=http://localhost:5173,http://localhost:9000,https://docs.medusajs.com
MEDUSA_BACKEND_URL=http://localhost:9000

DISABLE_MEDUSA_ADMIN=false
# Worker mode "shared" | "worker" | "server"
MEDUSA_WORKER_MODE=server
PORT=9000
# Example: postgres://username:password@host:5432/database_name
# If SSL is not supported, add ?sslmode=disable
# If SSL is required, add ?sslmode=require
DATABASE_URL= # POSTGRES DATABASE URL

# Redis
REDIS_URL= # REDIS DATABASE URL
CACHE_REDIS_URL= # REDIS CACHE URL
LOCKING_REDIS_URL= # REDIS LOCKING URL

# Database name (optional, used in some configs)
DB_NAME=medusa-v2

# S3 File Storage
S3_FILE_URL=https://domain.com
S3_ACCESS_KEY_ID=access_key_id
S3_SECRET_ACCESS_KEY=secret_access_key
S3_REGION=auto
S3_BUCKET=bucket_name
S3_ENDPOINT=https://endpoint
S3_PREFIX=TH_
